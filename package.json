{
  "name": "redyn",
  "version": "0.0.6",
  "description": "Promise-first Redis-implementation for NodeJS backed by DynamoDB",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js .",
    "test": "nyc mocha",
    "test:mocha": "mocha",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": "https://github.com/someimportantcompany/redyn",
  "bugs": "https://github.com/someimportantcompany/redyn/issues",
  "homepage": "https://someimportantcompany.com/redyn",
  "author": "James D <james@jdrydn.com> (https://jdrydn.com)",
  "license": "MIT",
  "_moduleAliases": {
    "redyn": "./"
  },
  "keywords": [
    "amazon",
    "aws",
    "dynamodb",
    "dynamo",
    "redis",
    "key-value"
  ],
  "files": [
    "*.js",
    "!*.test.js",
    "commands/*.js",
    "!commands/*.test.js"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.303.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "dotenv": "^6.2.0",
    "eslint": "^8.37.0",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^10.2.0",
    "module-alias": "^2.2.2",
    "ms": "^2.1.3",
    "nock": "^13.3.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=16.0",
    "npm": ">=8.0"
  },
  "mocha": {
    "exit": true,
    "recursive": true,
    "reporter": "spec",
    "require": [
      "module-alias/register",
      "./test/bootstrap.js"
    ],
    "ignore": [
      "*node_modules*"
    ],
    "spec": [
      "./{,!(node_modules)/**}/*.test.js"
    ]
  },
  "nyc": {
    "exclude": [
      "*.test.js",
      "**/*.test.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
